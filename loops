 import random
#randomly select one value and increament it by the given value
for _ in range(1,5):
  print(random.randrange(0,20,2))
#iterate through the given range and increament by the value given
for i in range(0,10,2):
  print(i)
#iterate inside the list to assign value
a=[i for i in range(1,6)]
print(a)
#iterate through the list
for i in a:
  print(f"i={i}")
i=1
#while loop
while i < 10:
  print(i)
  i+=1
